<h2 id="main-title">Log In</h2>
    <div class="login-f">
        <%= form_tag(sessions_path) do %>
            <%= label_tag "User Name"%> <br>
            <%= text_field_tag :username%><br>
            <%= label_tag "Password"%><br>
            <%= password_field_tag :password %><br>
            <%= submit_tag "Log In" %>
        <% end %>
    </div>

    <h4 class="newtagline">New to Youth Juice?</h4>
<div class="button-area">
<%= link_to "Click Here to Sign Up", new_user_path, :class=> 'btn btn-secondary'  %>
</div>
</div>