<h2>Log In</h2>
    <%= flash[:notice] %>
<div class="form">
    <%= form_tag(sessions_path) do %>
        <%= label_tag "User Name" %>
        <%= text_field_tag :username %><br>
        <%= label_tag "Password" %>
        <%= password_field_tag :password %><br>
        <%= submit_tag "Log In" %>
    <% end %>
</div>